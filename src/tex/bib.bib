	note = {arXiv:1111.4246 [cs, stat]},
	keywords = {Statistics - Computation, Computer Science - Machine Learning},
	file = {arXiv Fulltext PDF:/Users/amandafarah/Zotero/storage/6T9CB2LE/Hoffman and Gelman - 2011 - The No-U-Turn Sampler Adaptively Setting Path Len.pdf:application/pdf;arXiv.org Snapshot:/Users/amandafarah/Zotero/storage/IVX2RSQ6/1111.html:text/html},
