	keywords = {1611, 162, 1900, 1925, 675, 677, 678, 98, Astrophysical black holes, Astrophysics - Cosmology and Nongalactic Astrophysics, Astrophysics - High Energy Astrophysical Phenomena, Bayesian statistics, Black holes, General Relativity and Quantum Cosmology, Gravitational wave astronomy, Gravitational wave sources, Gravitational waves, Hierarchical models, Stellar mass black holes},
	pages = {107},
