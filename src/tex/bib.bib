@article{ezquiaga_spectral_2022,
	title = {Spectral sirens: cosmology from the full mass distribution of compact binaries},
	volume = {129},
	issn = {0031-9007, 1079-7114},
	shorttitle = {Spectral sirens},
	url = {http://arxiv.org/abs/2202.08240},
	doi = {10.1103/PhysRevLett.129.061102},
	abstract = {We explore the use of the mass spectrum of neutron stars and black holes in gravitational-wave compact binary sources as a cosmological probe. These standard siren sources provide direct measurements of luminosity distance. In addition, features in the mass distribution, such as mass gaps or peaks, will redshift, and thus provide independent constraints on their redshift distribution. We argue that the entire mass spectrum should be utilized to provide cosmological constraints. For example, we find that the mass spectrum of LIGO--Virgo--KAGRA events introduces at least five independent mass ``features'': the upper and lower edges of the pair instability supernova (PISN) gap, the upper and lower edges of the neutron star--black hole gap, and the minimum neutron star mass. We find that although the PISN gap dominates the cosmological inference with current detectors (2G), as shown in previous work, it is the lower mass gap that will provide the most powerful constraints in the era of Cosmic Explorer and Einstein Telescope (3G). By using the full mass distribution, we demonstrate that degeneracies between mass evolution and cosmological evolution can be broken, unless an astrophysical conspiracy shifts all features of the full mass distribution simultaneously following the (non-trivial) Hubble diagram evolution. We find that this self-calibrating ``spectral siren'' method has the potential to provide precision constraints of both cosmology and the evolution of the mass distribution, with 2G achieving better than \$10{\textbackslash}\%\$ precision on \$H(z)\$ at \$z{\textbackslash}lesssim1\$ within a year, and 3G reaching \${\textbackslash}lesssim1{\textbackslash}\%\$ at \$z{\textbackslash}gtrsim2\$ within one month.},
	number = {6},
	urldate = {2022-11-20},
	journal = {Physical Review Letters},
	author = {Ezquiaga, Jose María and Holz, Daniel E.},
	month = aug,
	year = {2022},
	note = {arXiv:2202.08240 [astro-ph, physics:gr-qc]},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, General Relativity and Quantum Cosmology, Astrophysics - Cosmology and Nongalactic Astrophysics},
	pages = {061102},
	file = {arXiv Fulltext PDF:/Users/amandafarah/Zotero/storage/R5WRYHYP/Ezquiaga and Holz - 2022 - Spectral sirens cosmology from the full mass dist.pdf:application/pdf;arXiv.org Snapshot:/Users/amandafarah/Zotero/storage/ANZF35KW/2202.html:text/html},
}

@article{messenger_measuring_2012,
	title = {Measuring a {Cosmological} {Distance}-{Redshift} {Relationship} {Using} {Only} {Gravitational} {Wave} {Observations} of {Binary} {Neutron} {Star} {Coalescences}},
	volume = {108},
	issn = {0031-9007},
	url = {https://ui.adsabs.harvard.edu/abs/2012PhRvL.108i1101M},
	doi = {10.1103/PhysRevLett.108.091101},
	abstract = {Detection of gravitational waves from the inspiral phase of binary neutron star coalescence will allow us to measure the effects of the tidal coupling in such systems. Tidal effects provide additional contributions to the phase evolution of the gravitational wave signal that break a degeneracy between the system’s mass parameters and redshift and thereby allow the simultaneous measurement of both the effective distance and the redshift for individual sources. Using the population of O(103-107) detectable binary neutron star systems predicted for 3rd generation gravitational wave detectors, the luminosity distance-redshift relation can be probed independently of the cosmological distance ladder and independently of electromagnetic observations. We conclude that for a range of representative neutron star equations of state the redshift of such systems can be determined to an accuracy of 8\%-40\% for z{\textless}1 and 9\%-65\% for 1},
	urldate = {2022-12-16},
	journal = {Physical Review Letters},
	author = {Messenger, C. and Read, J.},
	month = mar,
	year = {2012},
	note = {ADS Bibcode: 2012PhRvL.108i1101M},
