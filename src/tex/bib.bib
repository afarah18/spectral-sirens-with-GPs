	journal = {The Astronomical Journal},
	author = {Gair, Jonathan R. and Ghosh, Archisman and Gray, Rachel and Holz, Daniel E. and Mastrogiovanni, Simone and Mukherjee, Suvodip and Palmese, Antonella and Tamanini, Nicola and Baker, Tessa and Beirnaert, Freija and Bilicki, Maciej and Chen, Hsin-Yu and Dálya, Gergely and Ezquiaga, Jose Maria and Farr, Will M. and Fishbach, Maya and Garcia-Bellido, Juan and Ghosh, Tathagata and Huang, Hsiang-Yu and Karathanasis, Christos and Leyde, Konstantin and Magaña Hernandez, Ignacio and Noller, Johannes and Pierra, Gregoire and Raffai, Peter and Romano, Antonio Enea and Seglar-Arroyo, Monica and Steer, Danièle A. and Turski, Cezary and Vaccaro, Maria Paola and Vallejo-Peña, Sergio Andrés},
