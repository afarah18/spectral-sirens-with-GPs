	keywords = {Astrophysics - High Energy Astrophysical Phenomena, General Relativity and Quantum Cosmology},
	annote = {Comment: 24 pages, 14 figures; code can be found at http://github.com/tcallister/autoregressive-bbh-inference and data can be download from https://zenodo.org/record/7616096},
	file = {arXiv Fulltext PDF:/Users/amandafarah/Zotero/storage/FS46GUJX/Callister and Farr - 2023 - A Parameter-Free Tour of the Binary Black Hole Pop.pdf:application/pdf;arXiv.org Snapshot:/Users/amandafarah/Zotero/storage/TI8E73SX/2302.html:text/html},
